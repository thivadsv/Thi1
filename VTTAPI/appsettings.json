{
  // KHỐI 1: Cấu hình Logging
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }, // <--- PHẢI CÓ DẤU PHẨY NẾU CÓ KHỐI KHÁC PHÍA SAU

  // KHỐI 2: Cấu hình ConnectionStrings (NGANG HÀNG với Logging)
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=VTTECHDB_Example;Trusted_Connection=True;"
  },

  // KHỐI 3: AllowedHosts (NGANG HÀNG với Logging và ConnectionStrings)
  "AllowedHosts": "*"
}